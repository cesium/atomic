<div class="divide60">
</div>
<div class="container">
  <div class="row">
    <div class="col-md-2 col-md-offset-5">
      <%= image_tag @user.image, class: 'img-responsive img-circle' %>
    </div>
  </div>
  <br />
  <div class="row">
    <div class="col-md-12 text-center">
      <h3>
        <%= @user.name %>
        <br />
        <small class="text-muted">
          <%= @user.email %>
        </small></h3>
      <% if @user.member_id %>
      <div class="btn btn-success nohover">
        Member
      </div>
    <% end %></div>
</div>
</div>
<div class="divide60">
</div>